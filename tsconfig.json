{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@services/*": ["services/*"],
      "@store/*": ["store/*"],
      "@utils/*": ["utils/*"],
      "@config/*": ["config/*"],
      "@database/*": ["database/*"],
      "@types": ["types/index"],
      "@assets/*": ["../assets/*"]
    }
  }
}
